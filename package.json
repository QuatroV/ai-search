{
  "name": "tour-parser-api",
  "version": "1.0.0",
  "description": "API that converts natural language tour requests to structured data using Ollama",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "client": "cd client && npm start",
    "client-install": "cd client && npm install",
    "build": "cd client && npm run build",
    "dev-full": "concurrently \"npm run dev\" \"npm run client\"",
    "install-all": "npm install && npm run client-install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "body-parser": "^1.20.2",
    "node-fetch": "^2.6.9"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.0.1"
  }
} 